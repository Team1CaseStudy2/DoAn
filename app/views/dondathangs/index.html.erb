<p id="notice"><%= notice %></p>

<h1>Dondathangs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dondathangs.each do |dondathang| %>
      <tr>
        <td><%= link_to 'Show', dondathang %></td>
        <td><%= link_to 'Edit', edit_dondathang_path(dondathang) %></td>
        <td><%= link_to 'Destroy', dondathang, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dondathang', new_dondathang_path %>
